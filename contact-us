    #email-form > [id*='step'] {
        display: none;
    }

    #email-form > [id*='step'].shown {
        display: flex;
        opacity: 0;
        animation: fade-in 200ms ease-in forwards;
    }

    @keyframes fade-in {
        from {
            opacity: 0;
        }

        to {
            opacity: 1;
        }
    }

    .step-child.flow0 {
        transform: translateX(0%);
    }

    .step-child.flow1 {
        transform: translateX(-100%);
    }

    .step-child.flow2 {
        transform: translateX(-200%);
    }
    .hidden {
        display: none;
    }
</style>

<script>
    (function leadForm() {
        const select = (selector, parent = document) => parent.querySelector(selector);
        const selectAll = (selector, parent = document) => [...parent.querySelectorAll(selector)];

        let emailForm = select('#email-form');
        // [help, options, budget, projectDetails, letsTalk]
        let steps = selectAll('#email-form > [id*=step]');

        let formNav = select('#email-form .form-nav');
        let nextButton = select('#next-button', formNav);
        let prevButton = select('#prev-button', formNav);
        let prevButton2 = select('#prev-button-2', emailForm);
        let submitButton = select('#submit', emailForm);

        // show the first step
        steps[0].classList.add('shown');

        // increment as the user goes through the flow
        let currentStep = 0;

        let helpWith = ['website', 'marketing', 'brand'];

        // will be set based on whatever the user selects on step0 (help with)
        let flow = -1;

        const changeStep = ({ back = false } = {}) => {
            let justChat = flow === -1;
            if (back && currentStep > 0) {
                steps[currentStep].classList.remove('shown');

                // If they just want to chat, it means they should go back to step0
                let prevStep = justChat ? 0 : currentStep - 1;

                steps[prevStep].classList.add('shown');
                next(prevStep);
                currentStep = prevStep;
                if (currentStep === 3) {
                    formNav.classList.remove('hidden');
                } else {
                    formNav.classList.remove('hidden');
                }
                return;
            } else {
                // form validation before moving to the next step
                let stepFields = selectAll('input', steps[currentStep]);
                if (!stepFields.every((input) => input.checkValidity())) {
                    return stepFields.every((input) => input.reportValidity());
                }

                steps[currentStep].classList.remove('shown');

                // If they just want to chat take them directly to project details
                let nextStep = justChat ? (currentStep < 3 ? 3 : currentStep + 1) : currentStep + 1;

                steps[nextStep].classList.add('shown');
                next(nextStep);
                currentStep = nextStep;
            }
        };

        nextButton.addEventListener('click', () => changeStep());
        prevButton.addEventListener('click', () => changeStep({ back: true }));
        prevButton2.addEventListener('click', () => changeStep({ back: true }));

        const onChange = (step) => {
            let ran = false;
            return (ev) => {
                let choice = ev.target.value;
                if (step === 0) {
                    // step-0 dictates the options and the budgets
                    flow = helpWith.indexOf(choice);
                    if (flow !== -1) {
                        steps.slice(1, 3).forEach((step) => {
                            selectAll('.step-child', step).forEach((child) => {
                                // clean up classes from previous flows
                                child.classList.remove('flow0', 'flow1', 'flow2');
                                // translate the correct options into view
                                child.classList.add(`flow${flow}`);
                            });
                        });
                    }
                }

                if (!ran) {
                    if (currentStep == 0 || currentStep == 1) {
                        changeStep();
                    }
                    ran = true;
                }
            };
        };
        // prepare inputs to move to the next step on change
        const next = (step) => {
            // hide prev button on the first step
            if (step == 0) {
                prevButton.classList.add('hidden');
            } else {
                prevButton.classList.remove('hidden');
            }

            // hide next button on the frist two steps
            if (step == 0 || step == 1) {
                nextButton.classList.add('hidden');
            } else {
                nextButton.classList.remove('hidden');
            }

            let handler = onChange(step);
            selectAll('input', steps[step]).forEach((input) => {
                input.addEventListener('click', handler, { once: true });
            });

            // hide next button and let submit button take over
            if (step == 3) {
                formNav.classList.add('hidden');
            }
        };

        // enable next button for the first step
        next(0);
    })();
</script></div></form><div class="success-message w-form-done"><div class="div-block-117"><h2 class="heading-large text-color-white">We&#x27;ve received your submission!</h2></div></div><div class="w-form-fail"><div>Oops! Something went wrong while submitting the form.</div></div></div></div></div></main><div class="footer"><div class="page-padding"><div class="padding-vertical padding-huge"><div class="container-large"><div class="w-layout-grid grid-19"><div class="max-width-medium"><img src="https://assets-global.website-files.com/6287aed1aee28c153ef35b07/63eca44ad91d2ca2d9a8cf2e_algofy.svg" loading="lazy" width="250" height="Auto" alt="" class="image-45"/><div class="max-width-small margin-vertical margin-small text-font-montserrat-normal text-color-white">Algofy, digital experts motivated by your growth.</div><div class="max-width-small margin-vertical margin-small text-font-montserrat-normal text-color-white"><a href="mailto:contact@algofy.com">contact@algofy.com</a></div><div class="flex-horizontal-left"><a href="https://www.linkedin.com/company/algofy" class="link-block-3 link-blue w-inline-block"><div class="w-embed"><svg xmlns="http://www.w3.org/2000/svg" fill="CurrentColor" viewBox="0 0 50 50" width="50px" height="50px">    <path d="M41,4H9C6.24,4,4,6.24,4,9v32c0,2.76,2.24,5,5,5h32c2.76,0,5-2.24,5-5V9C46,6.24,43.76,4,41,4z M17,20v19h-6V20H17z M11,14.47c0-1.4,1.2-2.47,3-2.47s2.93,1.07,3,2.47c0,1.4-1.12,2.53-3,2.53C12.2,17,11,15.87,11,14.47z M39,39h-6c0,0,0-9.26,0-10 c0-2-1-4-3.5-4.04h-0.08C27,24.96,26,27.02,26,29c0,0.91,0,10,0,10h-6V20h6v2.56c0,0,1.93-2.56,5.81-2.56 c3.97,0,7.19,2.73,7.19,8.26V39z"/></svg></div></a></div><div class="margin-top margin-medium"><div class="footer-awards_wrapper"><div class="w-layout-grid footer-awarods_layout"><div class="html-embed-9 w-embed"><a href="https://www.google.com/partners/agency?id=6689854360" target="_blank">
