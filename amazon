  <!-- Performance pricing calculator -->
  <script>
    var rangeValSpend = 0;
    var rangeValROAS = 0;


    function pf4_service_page_calculator_data_count(monthly_ad_spend, refund_ad_spend) {
      var monthly_ad_spend_num = Number(monthly_ad_spend);
      var refund_ad_spend_num = Number(refund_ad_spend);

      // calculate amounts
      var price = Math.floor((monthly_ad_spend_num * 0.2) * (refund_ad_spend_num / 8));
      var revenue = Math.floor(monthly_ad_spend_num * refund_ad_spend_num);

      if (price < 500) {
        price = 500;
      }
      if (revenue < 1) {
        revenue = 0;
      }

      // calculate percentages
      var total = (monthly_ad_spend_num + price + revenue);
      var bar_percentage_ad_spend = ((monthly_ad_spend_num / total) * 100);
      var bar_percentage_our_cost = ((price / total) * 100);
      var bar_percentage_revenue = (100 - (bar_percentage_ad_spend + bar_percentage_our_cost));

      // truncate calculated percentages to 2 decimal places
      bar_percentage_ad_spend = (Math.round(bar_percentage_ad_spend * 100) / 100);
      bar_percentage_our_cost = (Math.round(bar_percentage_our_cost * 100) / 100);
      bar_percentage_revenue = (Math.round(bar_percentage_revenue * 100) / 100);

      if (monthly_ad_spend_num === 0) {
        bar_percentage_our_cost = 100;
      }
      if (monthly_ad_spend_num === 0) {
        bar_percentage_revenue = 0;
      }

      // update text elements
      $('#display--costs').text('$' + price);
      $('#display--costs').css('color', (price > 0) ? '#eaf0f3' : '');

      $('#display--revenue').text('$' + revenue);
      $('#display--revenue').css('color', (revenue > 0) ? '#eaf0f3' : '');

      // show / hide donut chart elements based on value
      $('.donut-segment--costs').css('display', (bar_percentage_our_cost === 0) ? 'none' : 'block');
      $('.donut-segment--spend').css('display', (bar_percentage_ad_spend === 0) ? 'none' : 'block');
      $('.donut-segment--revenue').css('display', (bar_percentage_revenue === 0) ? 'none' : 'block');

      // update donut chart element sizes
      $('.donut-segment--spend').attr('stroke-dasharray', (bar_percentage_revenue + bar_percentage_ad_spend) + ' ' + 100);
      $('.donut-segment--revenue').attr('stroke-dasharray', bar_percentage_revenue + ' ' + (100 - bar_percentage_revenue));
    }


    function calculate(currentInput) {
      var currentInputId = currentInput.attr('id');

      // find out which sliders value is updated, and store appropriate value
      if (currentInputId === 'range-slider--spend') {
        rangeValSpend = parseInt(currentInput.val(), 10);

      } else if (currentInputId === 'range-slider--roas') {
        rangeValROAS = parseFloat(currentInput.val());
      }

      // calculate / update value displays
      pf4_service_page_calculator_data_count(rangeValSpend, rangeValROAS);
    }


    function applyFill(slider) {
      var sliderEl = slider.get(0);

      // Let's turn our value into a percentage to figure out how far it is in between the min and max of our input
      var percentage = 100 * (sliderEl.value - sliderEl.min) / (sliderEl.max - sliderEl.min);

      // now we'll create a linear gradient that separates at the above point
      // Our background color will change here
      sliderEl.style.background = 'linear-gradient( 90deg, #b5b5b5 ' + percentage + '% , #EAF0F3 ' + percentage + '%)';
    }




    $(document).ready(function ($) {
      $('.range-slider').on('input', function () {
        var slider = $(this);
        var inputField = slider.parents('.calculator-block').find('.input-slider-field');

        // update input slider display
        inputField.val(
          slider.val()
        );

        // apply our fill to the slider
        applyFill(
          slider
        );

        // update calculation / display
        calculate(
          slider
        );
      });

      $('.input-slider-field').on('change', function () {
        var inputField = $(this);
        var slider = inputField.parents('.calculator-block').find('.range-slider');

        // update slider display
        slider.val(
          inputField.val()
        );

        // apply our fill to the slider
        applyFill(
          slider
        );

        // update calculation / display
        calculate(
          slider
        );
      });

      // set initial state
      $('.range-slider').trigger('input');
    });

  </script>
   <!-- Performance pricing calculator -->
