var words = document.getElementsByClassName('word');
var wordArray = [];
var currentWord = 0;

words[currentWord].style.opacity = 1;
for (var i = 0; i < words.length; i++) {
  splitLetters(words[i]);
}

function changeWord() {
  var cw = wordArray[currentWord];
  var nw = currentWord == words.length-1 ? wordArray[0] : wordArray[currentWord+1];
  for (var i = 0; i < cw.length; i++) {
    animateLetterOut(cw, i);
  }
  
  for (var i = 0; i < nw.length; i++) {
    nw[i].className = 'letter behind';
    nw[0].parentElement.style.opacity = 1;
    animateLetterIn(nw, i);
  }
  
  currentWord = (currentWord == wordArray.length-1) ? 0 : currentWord+1;
}

function animateLetterOut(cw, i) {
  setTimeout(function() {
		cw[i].className = 'letter out';
  }, i*80);
}

function animateLetterIn(nw, i) {
  setTimeout(function() {
		nw[i].className = 'letter in';
  }, 340+(i*80));
}

function splitLetters(word) {
  var content = word.innerHTML;
  word.innerHTML = '';
  var letters = [];
  for (var i = 0; i < content.length; i++) {
    var letter = document.createElement('span');
    letter.className = 'letter';
    letter.innerHTML = content.charAt(i);
    word.appendChild(letter);
    letters.push(letter);
  }
  
  wordArray.push(letters);
}

changeWord();
setInterval(changeWord, 4000);
</script>

<style>
.letter {
  display: inline-block;
  position: relative;
  float: left;
  transform: translateZ(25px);
  transform-origin: 50% 50% 25px;
}

.letter.out {
  transform: rotateX(90deg);
  transition: transform 0.32s cubic-bezier(0.55, 0.055, 0.675, 0.19);
}

.letter.behind {
  transform: rotateX(-90deg);
}

.letter.in {
  transform: rotateX(0deg);
  transition: transform 0.38s cubic-bezier(0.175, 0.885, 0.32, 1.275);
}
</style></div><div class="hover-slider w-embed w-script"><style>
    :root {
        /* you should change the value of this variable
         * if you give the grid a new gap value
         */
        --hover-scroll-gap: 2rem;
    }

    .hover-slider-target {
        transition: transform 500ms ease-in-out;
    }
    
    .hover-slider-target.selected {
    		-webkit-transform: scale(1.1);
        -ms-transform: scale(1.1);
        transform: scale(1.1);
        /* either add specifi styles here for the selected slider tartet
       * or implement them using the .selected combo class
       */
    }
</style>

<script>
    function hoverSlider(section) {
        if (!section) {
            throw new Error('==== Missing section parameter to hover slider ====');
        }
        const hoverSlideTrigger = Array.from(document.querySelectorAll([section, '.hover-slider-trigger'].join(' ')));
        const hoverSlideTargets = Array.from(document.querySelectorAll([section, '.hover-slider-target'].join(' ')));
				const numItems = hoverSlideTargets.length;

				const container = hoverSlideTargets[0].parentElement;
        const containerWidth = container.offsetWidth;
        const containerScrollWidth = container.scrollWidth;
        const itemWidth = hoverSlideTargets[0].offsetWidth;
				
        // a variable to hold the original scroll target animation so that
        // we only have to define it once in the CSS
        let targetAnimation = `infinite-scroll-${section.slice(1)} ${7.5 * numItems}s linear infinite alternate`;
        // timer use to restart the animation on target mouseleave
        // gives the user enough time to hover on another trigger
        // to avoid jumping
        let timerId = null;

        //TODO: on mouse leave, add the inifinite animation back
        hoverSlideTrigger.forEach((trigger) => {
            trigger.addEventListener('mouseenter', (ev) => {
                // remove any pending animation secheduling
                clearTimeout(timerId);

                let slideTargetIndex = hoverSlideTrigger.findIndex((trigger) => trigger === ev.target);
                hoverSlideTargets.forEach((target, targetIndex) => {
                    target.classList.toggle('selected', targetIndex === slideTargetIndex);

                    target.style.animation = 'none';
                    let containerWidth = target.parentElement?.offsetWidth || 0;
                    // half the container width minus half the target width to center it
                    // minus the target's offsetLeft to get the correct position accounting for the items left siblings
                    target.style.transform = `
           translateX(calc(${containerWidth / 2}px
             - ${itemWidth / 2}px
              - calc(${slideTargetIndex} * calc(${itemWidth}px + var(--hover-scroll-gap)))
             )
              )`;
                });
            });
        });

        const styleSheet = document.createElement('style');
        document.head.appendChild(styleSheet);
				debugger;
        styleSheet.sheet?.insertRule(
            `@keyframes infinite-scroll-${section.slice(1)} {
         0% {
             transform: translateX(0px);
         }
         100% {
           transform: translateX(${-containerScrollWidth + containerWidth}px);
         }
     }`
        );
      
        hoverSlideTargets.forEach((target) => {
            target.style.animation = targetAnimation;
        });
      
        hoverSlideTrigger.forEach((trigger) => {
            trigger.addEventListener('mouseleave', () => {
                hoverSlideTargets.forEach((target) => {
                    // remove the selected class immediately
                    target.classList.remove('selected');
                });
                // schedule targets for animation
                timerId = setTimeout(() => {
                    hoverSlideTargets.forEach((target) => {
                        target.style.animation = targetAnimation;
                    });
                }, 150);
            });
        });
    }

    [
     '#team-section',
     '#amazon-section'
    ].forEach((section) => hoverSlider(section));
</script></div></div><script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=6287aed1aee28c153ef35b07" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script><script src="https://assets-global.website-files.com/6287aed1aee28c153ef35b07/js/algfoy.c52d423dc.js" type="text/javascript"></script>   <!-- smooth scroll 


