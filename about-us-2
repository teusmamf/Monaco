$(document).ready(function(){
    var lastYear = "2017";
    $(".timeline_content-wrapper[year-content='"+lastYear+"']").css("visibility","visible");
    $(".timeline_content-wrapper[year-content='"+lastYear+"']").animate({opacity:1});
});

$(".timeline-trigger[year]").hover(function(){
    var year = $(this).attr("year");
    $(".timeline_content-wrapper").not("[year-content='"+year+"']").animate({opacity:0});
    $(".timeline_content-wrapper").not("[year-content='"+year+"']").css("visibility","hidden");
    $(".timeline_content-wrapper[year-content='"+year+"']").css("visibility","visible");
    $(".timeline_content-wrapper[year-content='"+year+"']").animate({opacity:1});
    $(this).addClass("active");
    $(".timeline-trigger[year]").not(this).removeClass("active");
});


</script></body></html>