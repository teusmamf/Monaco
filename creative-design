// when document is fully loaded
$( document ).ready(function() {

$('.list').slick({
  dots: false,
  speed: 600,
  infinite: true,
  slidesToShow: 3,
  slidesToScroll: 1,
  arrows: false,
  centerMode: true,
  touchThreshold:300,
  responsive: [
    {
      // tablet
      breakpoint: 991,
      settings: {
        slidesToShow: 2
      }
    },
    {
      // mobile portrait
      breakpoint: 479,
      settings: {
        slidesToShow: 1
      }
    }
  ]
});

$('.slider-prev').click(function(){
    $(this).closest('.section').find(".list").slick('slickPrev');
});

$('.slider-next').click(function(){
    $(this).closest('.section').find(".list").slick('slickNext');
});

});

</script>

